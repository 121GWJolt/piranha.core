@model IList<Piranha.Extend.PageType>
@{
    ViewBag.Title = "Page types";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</td>
            <th>Display name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
            </tr>
        }
    </tbody>
</table>
